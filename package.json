{
  "name": "gameshouse",
  "scripts": {
    "prettier": "prettier --write src",
    "eslint": "eslint --fix --ext .ts,.tsx src",
    "format": "npm run eslint && npm run prettier",
    "server": "tsc -p server/tsconfig.json",
    "dev": "vite",
    "build": "npm run format && tsc && vite build && npm run server",
    "start": "node express/server.js",
    "heroku-postbuild": "vite build && npm run server"
  },
  "dependencies": {
    "express": "^4.17.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "socket.io": "^4.3.1",
    "socket.io-client": "^4.3.2",
    "vite": "^2.6.13"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.3.1",
    "@types/styled-components": "^5.1.15",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vitejs/plugin-react": "^1.0.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.4.1",
    "react-router-dom": "^5.3.0",
    "styled-components": "^5.3.3",
    "typescript": "^4.3.2"
  },
  "engines": {
    "node": "v14.4.0"
  }
}
